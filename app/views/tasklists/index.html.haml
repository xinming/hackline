%h1 Listing tasklists

%table
  %tr
    %th Project
    %th Name
    %th Details
    %th
    %th
    %th

  - @tasklists.each do |tasklist|
    %tr
      %td= tasklist.project
      %td= tasklist.name
      %td= tasklist.details
      %td= link_to 'Show', tasklist
      %td= link_to 'Edit', edit_tasklist_path(tasklist)
      %td= link_to 'Destroy', tasklist, :confirm => 'Are you sure?', :method => :delete

%br

= link_to 'New Tasklist', new_tasklist_path
